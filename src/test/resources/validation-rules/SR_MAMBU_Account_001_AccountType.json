{
  "apiVersion": "0.3.0",
  "ruleVersion": 1,
  "name": "SR_MAMBU_Account_001_AccountType",
  "description": "MAMBU Account Types",
  "schemas": [
    {"name": "Account", "isParentSchema": true, "operationIds": ["updateAccount"]},
    {"name": "AccountRequest", "isParentSchema": true, "operationIds": ["openBankingAccount"]}
  ],  "conditions": {
    "all": [
      {
        "type": "JSON_LOGIC",
        "comment": "Account Type must be one of ['CLIENT', 'Checking']",
        "logic": {"in":[ { "var": "accountType" } , ["CLIENT", "Checking",null] ]}
      }
    ]
  },
  "event": {
    "comment": "Invalid Request Error",
    "type": "error",
    "target": "accountType",
    "code": "InvalidAccountType",
    "message": "Account type must be one of ['CLIENT', 'Checking'] "
  }
}